/**
 * 归并排序，时间复杂度为nlgn
 * @author sinllychen
 *
 */
public class MergeSort {
	public static void mergeSort(int[] k,int i,int j)
	{
		int q;
		if(i<j)
		{
			q=(int) Math.floor((i+j)/2);
			mergeSort(k,i,q);
			mergeSort(k,q+1,j);
		}
	}
    public static void main(String[] args)
    {
    	int a[]={5,2,6,7,1,3,9,0,10};
    	
    }
}
