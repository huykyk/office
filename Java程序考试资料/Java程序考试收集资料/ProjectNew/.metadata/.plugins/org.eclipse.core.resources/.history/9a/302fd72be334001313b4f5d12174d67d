import java.util.ArrayList;


public class Common {
	   /**
     * 交换两个数
     * @param datas
     * @param i
     * @param j
     */
    public static void swap(ArrayList<Integer> datas,int i,int j)
    {
        int k=datas.get(i);
        datas.set(i, datas.get(j));
        datas.set(j, k);
    }
}
